    "@nestjs/core": "8.0.6",
    "@nestjs/graphql": "8.0.2",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/passport": "8.0.1",
    "@nestjs/platform-express": "8.0.6",
    "@nestjs/typeorm": "8.0.2",
    "apollo-server-express": "2.25.2",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "graphql": "15.5.1",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "pg": "8.7.1",
    "rxjs": "7.3.0",
    "typeorm": "0.2.34",
    "typeorm-naming-strategies": "2.0.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@gemunionstudio/cross-env": "0.0.3",
    "@gemunionstudio/eslint-config": "0.1.8",
    "@nestjs/testing": "8.0.6",
    "@types/express": "4.17.13",
    "@types/jest": "26.0.24",
    "@types/node": "14.17.9",
    "@types/passport": "1.0.7",
    "@types/passport-jwt": "3.0.6",
    "@types/uuid": "8.3.1",
    "@typescript-eslint/eslint-plugin": "4.29.0",
    "@typescript-eslint/parser": "4.29.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-import-resolver-ts": "0.4.2",
    "eslint-plugin-import": "2.24.0",
    "eslint-plugin-jest": "24.4.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-standard": "4.1.0",
    "jest": "27.0.6",
    "prettier": "2.3.2",
    "ts-jest": "27.0.4",
    "ts-node": "10.2.0",
    "tsc-watch": "4.4.0",
    "typescript": "4.3.5"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "name": "@gemunionstudio/jwt-gql",
  "repository": {
    "type": "git",
    "url": "https://github.com/GemunIonStudio/nestjs-auth"
  },
  "scripts": {
    "build": "tsc",
    "entity:create": "npm run typeorm -- entity:create -n",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "migration:create": "npm run typeorm migration:create -- -n",
    "migration:generate": "npm run typeorm migration:generate -- -n",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:run": "npm run typeorm migration:run",
    "prebuild": "rm -rf dist/",
    "prod": "cross-env NODE_ENV=${NODE_ENV:=production} node .",
    "start": "cross-env NODE_ENV=${NODE_ENV:=development} tsc-watch --onSuccess \"node .\" --onFailure \"echo Compilation Failed\"",
    "test": "cross-env NODE_ENV=${NODE_ENV:=test} jest --config jest.config.json --detectOpenHandles",
    "typeorm": "cross-env NODE_ENV=${NODE_ENV:=development} ts-node ./node_modules/.bin/typeorm --config src/ormconfig.ts"
  },
  "title": "JWT + GraphQL based authorization for Nest.js",
  "version": "0.0.1"
}
